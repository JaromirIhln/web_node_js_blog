# IMPORTANT: You must change all values to match your own database and app settings.
# Do NOT use these example values in production!

# DB host (např. localhost nebo adresa serveru)
DB_HOST=localhost

# Název databáze
DB_NAME=your_db_name

# Port databáze (výchozí 5432)
DB_PORT=5432

# Uživatelské jméno do DB
DB_USER=your_db_user

# Heslo do DB (pokud obsahuje speciální znaky, použijte apostrofy)
DB_PASSWORD='your_db_password'

# Tajný klíč pro šifrování
SECRET_KEY=your_some_secret_key

# Port aplikace (výchozí 3000)
PORT=3000